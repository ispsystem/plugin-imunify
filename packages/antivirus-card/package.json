{
  "name": "antivirus-card",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/antivirus-card/antivirus-card.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "index.esm": "cat index.esm.js >> dist/esm/index.mjs",
    "build": "stencil build --docs && npm run index.esm",
    "start": "stencil build --dev --watch --serve",
    "watch:index": "reflex -s -r './dist/collection/index.js' cp './dist/collection/index.js' './www/build'",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "1.1.1",
    "workbox-build": "4.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/sass": "^1.0.0"
  }
}
